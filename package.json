{
  "name": "passionfruit",
  "version": "0.5.5",
  "description": "iOS app analyzer tool",
  "main": "appv2.js",
  "engines": {
    "node": ">= 8.5.0"
  },
  "scripts": {
    "prepare": "pushd gui && npm install && popd && npm run frida:compile",
    "prepublishOnly": "cd gui && npm run build",
    "lint": "npm run lint:agent && npm run lint:server",
    "lint:agent": "eslint agent/app",
    "lint:server": "eslint *.js && eslint lib",
    "start": "cross-env NODE_ENV=development nodemon -r esm .",
    "frida:watch": "webpack --config agent/webpack.config.js --watch --mode development",
    "frida:compile": "webpack --config agent/webpack.config.js --mode production",
    "test": "mocha --timeout 10000 --require esm",
    "coverage": "nyc npm test"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "gui/*",
      "agent/*",
      "node_modules/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chaitin/passionfruit.git"
  },
  "keywords": [
    "iOS",
    "frida"
  ],
  "bin": "bin/cli.js",
  "files": [
    "gui/index.html",
    "gui/static",
    "lib/",
    "bin/",
    "agent/dist"
  ],
  "author": "CodeColorist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaitin/passionfruit/issues"
  },
  "homepage": "https://github.com/chaitin/passionfruit#readme",
  "dependencies": {
    "colors": "^1.3.3",
    "frida": "^12.6.10",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "plist": "^3.0.1",
    "promise.prototype.finally": "^3.1.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-stream": "^0.9.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.25",
    "macho": "^1.3.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.6",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  }
}
